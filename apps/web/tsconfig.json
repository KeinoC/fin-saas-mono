{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@features/*": [
        "./features/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@lib/*": [
        "./lib/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "analytics": [
        "../../packages/analytics"
      ],
      "integrations": [
        "../../packages/integrations"
      ],
      "services": [
        "../../packages/services"
      ],
      "config": [
        "../../packages/config"
      ],
      "ui": [
        "../../packages/ui"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
